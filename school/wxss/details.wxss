  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}

@-webkit-keyframes likeanimate {
  0% {
      transform: translate(-50%,-50%) scale(1);
  }

  50% {
      transform: translate(-50%,-50%) scale(1.3);
  }

  to {
      transform: translate(-50%,-50%) scale(1);
  }
}

@keyframes likeanimate {
  0% {
      transform: translate(-50%,-50%) scale(1);
  }

  50% {
      transform: translate(-50%,-50%) scale(1.3);
  }

  to {
      transform: translate(-50%,-50%) scale(1);
  }
}

@-webkit-keyframes twinkleanimate {
  0% {
      transform: translate(-50%,-50%) scale(.1);
  }

  50% {
      transform: translate(-50%,-50%) scale(1);
      opacity: 1;
  }

  to {
      opacity: 0;
  }
}

@keyframes twinkleanimate {
  0% {
      transform: translate(-50%,-50%) scale(.1);
  }

  50% {
      transform: translate(-50%,-50%) scale(1);
      opacity: 1;
  }

  to {
      opacity: 0;
  }
}

