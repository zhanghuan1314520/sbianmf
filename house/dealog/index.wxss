@import "../../dfBaseWxss__/3.wxss";

@import "../../dfBaseWxss__/7.wxss";

button::after {
    border: none;
}
.activity_title_name {
    font-size: 28rpx;
    font-weight: bold;
    color: rgba(29, 30, 33, 1);
}
.activity_title {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 90rpx;
    padding: 0 30rpx;
}
.container {
    width: 100%;
    height: 100vh;
    background-color: #EAE9E9;
}

.container .list-content {
    padding: 20rpx;
    background-color: #ffffff;
    width: 100%;

    box-sizing: border-box;
}

.container .list-content .list-block {
    display: flex;
    justify-content: flex-start;
    font-size: 24rpx;
    color: #8A8D91;
    border-bottom: solid 1px #d6dae1;
    line-height: 80rpx;
    height: 80rpx;
    vertical-align: middle;
}

.container .list-content .block-head {
    font-size: 28rpx;
    height: 80rpx;
    font-weight: bold;
    color: #366AB3;
    background-color: #E7F4FA;
    display: flex;
    align-items: center;
    justify-content: center;
}

.container .list-content .load-more {
    font-size: 24rpx;
    text-align: center;
    color: #366AB3;
    padding: 20rpx;
}

.btns {
    height: 100%;
}

.btn {
    border-radius: 0;
    height: 100%;
    line-height: 90rpx;
    font-size: 28rpx;
    color: #fff;
    background-color: #366AB3;
    margin: 0;
    margin-left: 2rpx;
    width: 160rpx;
    border: 0;
}
.btn-warning{
  background-color: #d10e0e;
}
.fui-footer .tool {
    padding-right: 0;
}


 
.nav-bottom2 {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 14rpx 18rpx;
    position: fixed;
    left: 0;
    bottom: 0;
    background: #ffffff;
    width: 100%;
    box-sizing: border-box;
    /***border-top: 1px solid #000b2c;***/
    z-index: 99999999;
    
}

.nav-bottom2 .share {
    position: relative;
}

.nav-bottom2 .share button {
    
    left: 0;
    top: 0;
    display: block;
    width: 100%;
    height: 100%;
    opacity: 0;
}

 

.nav-bottom2 .guanzhu .images1 {
    width: 36rpx;
    height: 36rpx;
    display: block;
    margin-bottom: 6rpx;
}

.nav-bottom2 .guanzhu2 .images2 {
    width: 70rpx;
    height: 70rpx;
    display: block;
    margin-bottom: 6rpx;
    border-radius: 50rpx;
    

}

.nav-bottom2 .guanzhu1 .ntext {
    font-size: 30rpx;
    color: #366AB3;
    font-weight:bold;
}
.nav-bottom2 .guanzhu1 .ntext1 {
    font-size: 22rpx;  
}

.nav-bottom2 .guanzhu2.gz_img cover-image {
    width: 38rpx;
}

.nav-bottom2 .lianxi cover-image {
    width: 48rpx;
    height: 44rpx;
    display: block;
    margin-bottom: 4rpx;
}


.nav-bottom2 .fun {
    display: flex;
    align-items: center;
    font-size: 24rpx;
    
}

.nav-bottom2 .fun cover-view {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.nav-bottom2 .fun .guanzhu {
    width: 74rpx;
    padding-left: 10px;
    padding-right: 10px;
    box-sizing: content-box;
}
.nav-bottom2 .fun .guanzhu1 {
    width: 100rpx;
    padding-left: 5px;
    padding-right: 10px;
    box-sizing: content-box;
}
.nav-bottom2 .fun .guanzhu2 {
    width: 60rpx;
    padding-left: 10px;
    padding-right: 10px;
    box-sizing: content-box;
}

.nav-bottom2 .tell {
    width: 368rpx;
    height: 80rpx;
  
    border-radius: 54px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #fff;
 
    font-size: 30rpx;
    font-weight: 400;
    background-color: #0672ee;
}

.fix-iphonex-button {
    position: fixed;
    left: 0;
    bottom: 0!important;
    height: 68rpx!important;
    width: 100%;
    background: #fff;
    z-index: 9999999999999;
}


.mask_bg {
    position: fixed;
    left: 0;
    top: 0;
    display: block;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.5);
    z-index: 9999999;
}
.getphone_content {
    position: fixed;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    display: block;
    width: 560rpx;
    height: auto;
    padding: 40rpx 0 0;
    border-radius: 8rpx;
    background: rgba(255,255,255,1);
}

.getphone_content .info {
    display: flex;
    margin-bottom: 24rpx;
    padding: 0 24rpx;
}

.getphone_content .info cover-image {
    flex: 0 0 50rpx;
    height: 50rpx;
    border-radius: 50%;
    margin-right: 24rpx;

}

.getphone_content .info cover-view {
    display: inline-block;
    font-size: 28rpx;
    font-weight: 400;
    color: rgba(0,0,0,1);
    line-height: 30rpx;
    height: 50rpx;
    
}

.getphone_content .info cover-view text {
    font-weight: bold;
}

.getphone_content .desc {
    display: block;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(136,136,136,1);
    margin-top: 24rpx;
    padding: 0 24rpx;
    height: 200rpx;

}
.getphone_content .desc1 {
    display: block;
    font-size: 30rpx;
    font-weight: 400;
    color: rgba(136,136,136,1);
    margin-top: 10rpx;
    padding: 0 10rpx;
    height: 40rpx;

}

.getphone_content .btns {
    display: flex;
    border-top: 1rpx solid rgba(229,229,229,1);
}

.getphone_content .btns button {
    flex: 1;
    line-height: normal;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100rpx;
    font-size: 32rpx;
    font-weight: 400;
    background: #fff;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
}

.getphone_content .btns .cancel {
    border-bottom-right-radius: 0;
    color: rgba(102,102,102,1);
}    


/*学区成交明细标题*/
.schoollist {
   
    width: 100%;
    background: #fff;
}

.list__nav {
    width: 100%;
    font-size: 32rpx;
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1px solid rgba(51,51,51,.05);
    box-sizing: border-box;
}

.list__nav .list__nav__acitve {
    color: #1478ff;
    font-weight: 700;
}

.list__nav .list__nav__acitve:after {
    height: 6rpx;
    width: 60rpx;
    background: #1478ff;
    content: " ";
    display: block;
    position: absolute;
    bottom: 10rpx;
    left: 50%;
    margin: 0 auto 0 -30rpx;
}

.list__nav__item {
    position: relative;
    -webkit-box-flex: 1;
    flex: 1;
    text-align: center;
    font-weight: 700;
    font-size: 25rpx;
    color: #333;
}

.list__nav__item .nav__item__bg {
    width: 167rpx;
    height: 100rpx;
    position: absolute;
    right: 7rpx;
    opacity: .2;
    top: 6rpx;
}

.list__nav__item--share {
    width: 72rpx;
    margin-right: 24rpx;
}
