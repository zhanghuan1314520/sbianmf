}

.filter-info {
    height: 727rpx;
}

.filter-mask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 20000rpx;
    background: rgba(0, 0, 0, 0.6);
    z-index: 10;
}

.quyu-nav1, .quyu-nav2, .quyu-nav3 {
    position: absolute;
    top: 0;
    background: #fff;
}

.quyu-nav1 {
    width: 100%;
    background: #f3f3f3;
}

.quyu-nav2 {
    width: 74%;
    left: 26%;
}

.quyu-nav3 {
    width: 37%;
