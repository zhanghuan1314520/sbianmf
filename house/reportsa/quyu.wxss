}

.flex_column {
    flex-direction: column;
}

.flex_nowrap {
    flex-wrap: nowrap;
}

.flex_wrap {
    flex-wrap: wrap;
}

.flex_jus_start {
    justify-content: flex-start;
}

.flex_jus_center {
    justify-content: center;
}

.flex_jus_end {
    justify-content: flex-end;
}

.flex_jus_around {
