}

.house-list-box {
    font-size: 26rpx;
}

.list-top {
    width: 100%;
    background: #fff;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 11;
}

.search-box {
    background-color: #366ab3;
    padding: 20rpx;
    box-sizing: border-box;
}

.search-inp {
    background: #f9f9f9;
    border-radius: 40rpx;
    padding: 0 30rpx;
    position: relative;
}

.search-box .inp {
    height: 80rpx;
    font-size: 26rpx;
    background: #f9f9f9;
    line-height: 80rpx;
    width: 80%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #aaa;
}

.search-icon {
    width: 23rpx;
    height: 22rpx;
    display: inline-block;
    margin: 28rpx 20rpx 0 0;
}

.search-del {
    width: 34rpx;
    height: 34rpx;
    position: absolute;
    right: 30rpx;
    top: 11px;
    z-index: 2;
}

.filter-box {
    width: 100%;
    height: 70rpx;
    text-align: center;
    padding: 10rpx;
    border-bottom: 1rpx solid #fff;
    background-color: #fff;
    box-shadow: 1px 1px 1px #f3f2f2;
}

.filter-nav {
