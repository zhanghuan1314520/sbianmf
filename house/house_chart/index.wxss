.content {
  margin-bottom: 90rpx;
}
/* 搜索开始 */
.container-top {
  width: 100%;
  background-color: #366ab3;
  position: relative;
}

.container-top .shangbianju {
  height: 20rpx;
  width: 100%;
  border-radius: 20rpx 20rpx 0px 0rpx;
  background-color: white;
  position: absolute;
  bottom: 0;
}

.container-top .top-content {
  height: 80rpx;
  line-height: 80rpx;
  width: 90%;
  padding-left: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  background-color: #fff;
  color: #366ab3;
  border-bottom-right-radius: 20rpx;
  border-top-right-radius: 20rpx;
}

.container-top .top-content .select-bar {
  display: flex;
  padding-left: 45rpx;
  position: relative;
  background-color: #fff;
}

.container-top .top-content .select-bar::after {
  content: '';
  position: absolute;
  background-color: #366ab3;
  width: 8rpx;
  height: 100%;
  left: 20rpx;
}

.container-top .top-content .select-bar .house-catagory-select {
  font-size: 24rpx;
  border: 1px solid #366ab3;
  background-color: #366ab3;
  color: #fff;
  border-radius: 10rpx;
  height: 45rpx;
  line-height: 45rpx;
  width: 130rpx;
  text-align: center;
  margin-right: 20rpx;
}

.container-top .top-content .search-location {
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  text-align: center;
  font-size: 24rpx;
  overflow: hidden;
  height: 50rpx;
  vertical-align: middle;
  line-height: 50rpx;
}

.container-top .top-content .search {
  flex-grow: 1;
  margin: 15rpx 24rpx;
}
.search-box {
  background-color: #366ab3;
  padding: 20rpx;
  box-sizing: border-box;
}

.search-inp {
  background: #f9f9f9;
  border-radius: 40rpx;
  padding: 0 30rpx;
  position: relative;
}

.search-box .inp {
  height: 80rpx;
  font-size: 26rpx;
  background: #f9f9f9;
  line-height: 80rpx;
  width: 80%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #aaa;
}

.search-icon {
  width: 23rpx;
  height: 22rpx;
  display: inline-block;
  margin: 28rpx 20rpx 0 0;
}

.search-del {
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  right: 30rpx;
  top: 11px;
  z-index: 2;
}

.search {
  flex-grow: 1;
  /* margin: 15rpx 24rpx; */
  margin-bottom: 0rpx;

}


.search .serch_content {
  flex: 1;
  display: flex;
  align-items: center;
  border-radius: 15rpx;
  padding: 5rpx 5rpx;
  height: 65rpx;
  padding-left: 20rpx;

}

.search .serch_content .search_input {
  margin-left: 20rpx;
  font-size: 24rpx;
  width: 100%;
  background-color: #ffffff;
}
.search .serch_content .search_line{
  float:left;
  width: 2px;
  height: 60%; 
  background: #f1efef;
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.search .serch_content .search_map {
  margin-left: 5rpx;
  font-size:28rpx;
  font-weight: 600;
  color:#366AB3;
  width: 30%;
  background-color: #ffffff;
}
.search .btn_cancel {
  margin-left: 20rpx;
  width: 120rpx;
  color: #366ab3;
  font-size: 28rpx;
  line-height: 50rpx;
  height: 50rpx;
}
/* 搜索结束 */
.container-body {
  width: 710rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 20rpx;
  height: 860rpx;

