.house-content {
    height: 100%;
    background-color: #EAE9E9;
}

.house-content .block-head {
    margin-top: 20rpx;
    font-size: 28rpx;
    height: 80rpx;
    font-weight: bold;
    color: #366AB3;
    background-color: #E7F4FA;
    display: flex;
    align-items: center;
    justify-content: center;
}
.house-content .block-headz {
    margin-top: 20rpx;
    font-size: 28rpx;
    
    padding: 5rpx;
    font-weight: bold;
    color:rgb(223, 11, 11);
    background-color: #E7F4FA;
    display: flex;
    align-items: center;
    justify-content: center;
}

.house-content .house-body {
    padding: 20rpx;
    color: #8A8D91;
    background-color: #ffffff;
    box-sizing: border-box;
}

.house-content .house-body .house-body-header {
    font-size: 28rpx;
    display: flex;
    justify-content: space-between;
}

.house-content .house-body .house-body-header .house-isfirst {
    font-size: 36rpx;
    color: #366AB3;
    width:85%;
}

.house-content .house-body .house-body-item-content {
    margin: 20rpx;
    display: flex;
    flex-direction: column;
}

.house-content .house-body .house-body-item-content .house-body-item {
    display: flex;
    font-size: 24rpx;
    border-bottom: 2rpx solid #EAE9E9;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}

.house-content .house-body .house-body-item-content .house-body-item .house-body-item-block {
    flex: 1;
    display: flex;
}

.house-content .house-body .house-body-item-content .house-body-item .house-body-item-block .title {
    color: #366AB3;
    min-width: 150rpx;
}

.house-content .house-body .house-body-item-content .house-body-item .house-body-item-block .content {
    font-weight: bold;
}

.house-content .tiaojian {
    padding: 20rpx;
    display: flex;
    justify-content: space-between;
    background-color: #ffffff;
}

.house-content .tiaojian .select-date-content {
    font-size: 24rpx;
    color: #366AB3;
    display: flex;
    align-items: center;
}

.house-content .tiaojian .select-date-content .select-date {
    padding: 15rpx;
    background-color: #366AB3;
    color: #ffffff;
    border-radius: 10rpx;
}

.house-content .tiaojian .house-type-list {
    display: flex;
    align-items: center;
}

.house-content .tiaojian .house-type-list .house-type-item {
    padding: 15rpx;
    font-size: 24rpx;
    color: #366AB3;
    background-color: #ffffff;
    border: 1px solid #366AB3;
    border-radius: 10rpx;
    margin-right: 20rpx;
}

.house-content .tiaojian .house-type-list .house-type-item.item-acitve {
    color: #ffffff;
    background-color: #366AB3;
}

.house-content .rank-content {
    width: 100%;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    background-color: #ffffff;
    padding: 20rpx 50rpx 20rpx 50rpx;
}

.house-content .rank-content>view {
    display: flex;
    flex-direction: column;
}

.house-content .rank-content>view .top {
    font-size: 36rpx;
    color: #366AB3;
    text-align: center;
    font-weight:bold;
}

.house-content .rank-content>view .bottom {
    padding-top: 10rpx;
    font-size: 28rpx;
    color: #8A8D91;
    text-align: center;
}

.house-content .chart-body {
    margin-top: 20rpx;
    padding: 20rpx;
    box-sizing: border-box;
    background-color: #ffffff;
}

.house-content .chart-body .slot-title {
    font-size: 24rpx;
    font-weight: bold;
    margin-bottom: 10rpx;
}

.house-content .chart-body .slot-content .slot-content-item {
    color: #8A8D91;
    font-size: 24rpx;
    margin-top: 5rpx;
}

.house-content .deal-body {
    padding: 20rpx;
    color: #8A8D91;
    background-color: #ffffff;
    margin-top: 20rpx;
    padding-bottom: 100rpx;
    box-sizing: border-box;
}

.house-content .deal-body .deal-more {
    font-size: 24rpx;
    color: #8A8D91;
    float: right;
    height: 80rpx;
    line-height: 80rpx;
    vertical-align: middle;
}

.house-content .bottom-select-bar {
    height: 100rpx;
    width: 100%;
    background-color: #E7E7E8;
    position: fixed;
    display: flex;
    justify-content: space-around;
    left: 0;
    bottom: 0;
}

.house-content .bottom-select-bar .bottom-select-bar-item {
    display: flex;
    flex-direction: column;
    font-size: 24rpx;
    color: #5F5F60;
    justify-content: center;
    align-items: center;
}

.house-content .bottom-select-bar .bottom-select-bar-item .item-image {
    width: 50rpx;
    height: 50rpx;
    margin-bottom: 5rpx;
}
ec-canvas {
  width: 100%;
  height: 100%;
}
.house-content .right {
    flex: 1;
    overflow: hidden;
}

.house-content .right .top-bar {
    margin-top: 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 24rpx;
    background-color: #366AB3;
    color: #ffffff;
    padding: 20rpx;
    height: 30rpx;
}

.house-content .right .top-bar>view {
    flex: 1;
    text-align: center;
}

.house-content .right .right-content {
    display: flex;
    justify-content: space-between;
    position: relative;
    align-items: center;
    font-size: 24rpx;
    background-color: #ffffff;
    color: #8A8D91;
    border-bottom: #EAE9E9 1px solid;
    padding: 20rpx;
    height: 30rpx;
}

.house-content .right .right-content>view {
    flex: 1;
    text-align: center;
}

.house-content .right .right-content .goto-icon {
    position: absolute;
    right: 10rpx;    
}