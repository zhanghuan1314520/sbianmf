}

.container .container-top .item {
    min-width: 96rpx;
    text-align: center;
    display: flex;
    flex-direction: column;
}

.container .container-top .item .house-type-selected {
    font-weight: bold;
}

.container .container-top .item>image {
    width: 96rpx;
}

.container .container-top .item>view {
    font-size: 24rpx;
    height: 40rpx;
    line-height: 40rpx;
    color: #ffffff;
}

.slot-title {
    font-size: 24rpx;
    font-weight: bold;
