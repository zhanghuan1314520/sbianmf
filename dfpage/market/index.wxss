    box-sizing: border-box;
}

.container .container-body .data-content>view {
    flex: 1;
    color: #366AB3;
    display: flex;
    justify-content: center;
    flex-direction: column;
    align-items: center;
}

.container .container-body .data-content>view .data-content-num-topline {
