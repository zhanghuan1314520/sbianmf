    margin-top: 5rpx;
}

.container {
    width: 100%;
    height: 100%;
}

.container .container-top {
    width: 100%;
    background-color: #ffffff;
    position: relative;
}

.container .container-top .top-bar {
    height: 90rpx;
    line-height: 90rpx;
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #366AB3;
    font-size: 36rpx;
    padding-left: 20rpx;
}

.container .container-body {
