.content {
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  flex-direction: column;
  -webkit-box-align: center;
  align-items: center;
  -webkit-box-pack: center;
  justify-content: center;
}

.content,.header {
  position: relative;
}

.header {
  width: 100%;
  height: 364rpx;
  padding: 52rpx 40rpx 0;
  box-sizing: border-box;
}

.header .pageTitle {
  width: 100%;
  height: 88rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Semibold,PingFang SC;
  font-weight: 600;
  text-align: center;
  color: #fff;
  line-height: 88rpx;
  margin-bottom: 20rpx;
}

.header .address {
  margin-top: 50rpx;
  font-size: 40rpx;
  color: #fff;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.header .title {
  width: 100%;
  height: 80rpx;
  font-size: 32rpx;
  font-weight: 600;
  color: #000;
  line-height: 80rpx;
  text-align: center;
  position: relative;
}

.header .title .left {
  width: 40rpx;
  height: 40rpx;
  top: 20rpx;
  left: 0;
  position: absolute;
}

.body {
  width: 100%;
  height: 510rpx;
  background: #fff;
  /* border-radius: 40rpx; */
  border-radius: 40rpx 40rpx 0 0 ;
  position: absolute;
  top: 220rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
  /* bottom: 0; */
}

.body .t-table {
  margin-top: 40rpx;
}

.body .floorSelect {
  margin-top: 40rpx;
  width: 100%;
  height: 88rpx;
  background: #fff;
  display: flex;
}

.body .footer-radio {
  font-size: 32rpx;
  font-weight: 400;
  color: #595f66;
  margin-right: 40rpx;
}

.body .floor-active {
  height: 38rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Semibold,PingFang SC;
  font-weight: 600;
  color: #000;
  line-height: 38rpx;
  position: relative;
}

.body .floor-active::after {
  content: "";
  position: absolute;
  bottom: -24rpx;
  left: 32%;
  width: 48rpx;
  height: 8rpx;
  background: #0940ff;
  border-radius: 6rpx;
}

.body .footer-data {
  height: 400rpx;
}

.searchResult t-tr t-th {
  background: #f4f8fe;
  box-shadow: 0 2rpx 0 0 #e0ecff,0 -2rpx 0 0 #e0ecff;
  font-size: 28rpx;
  font-weight: 400;
  color: #707780;
}

.searchResult t-tr t-td {
  background: #fff;
  box-shadow: 0 -2rpx 0 0rpx #e0ecff;
}

.searchResult t-tr t-td .t-td {
  padding: 14rpx 0;
}

.search {
  position: relative;
  display: flex;
  -webkit-box-pack: center;
  justify-content: center;
  width: 100%;
  margin-bottom: 30rpx;
}

.search .icon-search {
  position: absolute;
  top: 20rpx;
  left: 3%;
}

.search .search-input {
  width: 670rpx;
  border-radius: 36rpx;
  height: 72rpx;
  background: #f1f3f5;
  opacity: .8;
  padding: 0 72rpx;
  box-sizing: border-box;
  font-size: 26rpx;
}

.footer {
  /* min-height: 500rpx; */
  height: auto;
  background: #f6f7f8;
  margin-top: 380rpx;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  flex-direction: column;
  padding: 0 40rpx;
  box-sizing: border-box;
}

.footer,.footer .title {
  width: 100%;
  display: flex;
}

.footer .title {
  -webkit-box-pack: center;
  justify-content: center;
  -webkit-box-align: center;
  align-items: center;
  height: 108rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #000;
}

.footer .sigle {
  margin-bottom: 40rpx;
  width: 100%;
  background: #fff;
  border-radius: 8px;
  padding: 32rpx 40rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.footer .sigle .icon-right {
  width: 48rpx;
  height: 48rpx;
  top: 57%;
  margin-top: -24rpx;
  right: 20rpx;
}

.footer .sigle .block {
  width: 540rpx;
  /* background-color: rgb(202, 126, 126); */
  /* display: flex; */
  /* margin-bottom: 16rpx; */
}

.footer .sigle .block .block-left {
  width: 540rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 400;
   color: #000;
  line-height: 40rpx;
}

 .block-right {
  width: 50rpx;
  height: 50rpx;
  /* background-color: rgb(79, 218, 199); */
}
.block-right image{
  width: 100%;
  height: 100%;
}

.footer .sigle .block .block-right .r-block {
  height: 40rpx;
  line-height: 40rpx;
}

.footer .sigle .block .block-right.blue {
  color: #00bfbf;
}
