/* project/pages/add/add.wxss */
page{
  background-color: #fff;
}
.search-box {
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  margin: 20rpx;
  font-size: 26rpx;
}

.search-box_l {
  width: 90%;
  display: flex;
  padding: 5rpx 10rpx;
  background-color: #f2f2f2;
  border-radius: 10rpx;
}

.search-box_l input {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 10rpx;
}

.search-box_r {
  margin-left: 20rpx;
  width: 10%;
}

.search-box_l image {
  width: 40rpx;
  height: 40rpx;
}